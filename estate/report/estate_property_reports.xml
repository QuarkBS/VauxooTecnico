<odoo>
    <record id="action_report_estate_properties" model="ir.actions.report">
        <field name="name">Property offers summary</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.report_property_offers</field>
        <field name="report_file">estate.report_property_offers</field>
        <field name="print_report_name">'Property offers'</field>
        <field name="binding_model_id" ref="model_estate_property"/>
        <field name="binding_type">report</field>
    </record>
    
    <record id="action_report_salesperson_properties" model="ir.actions.report">
        <field name="name">Salesperson properties summary</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.report_salesperson_properties</field>
        <field name="report_file">estate.report_salesperson_properties</field>
        <field name="print_report_name">'Salesperson offers'</field>
        <field name="binding_model_id" ref="base.model_res_users"/>
    </record>
</odoo>